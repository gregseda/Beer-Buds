<script>

function mySearch() 
{
    var input = document.getElementById('value').value;
    if (input == 'lager') 
    {
        window.location.assign('http://cis444.cs.csusm.edu/group_B/lager.php');
    }
    else 
    {
        alert("try again");
        document.getElementById('error').innerHTML = "try again";
    }
}


